package main

import "fmt"

func main() {
	for i := 65; i < 98; i++ {
		fmt.Printf("%d\n", i)
		for j := 0; j < 3; j++ {

			fmt.Printf("%#U\n", i)

		}
 
	}

	fmt.Printf("\n")
	favSport := "TT"
	switch favSport {

	case "TT":
		fmt.Println("yes you are there")
	case "Cricket":
		fmt.Println("No here")
	}
	fmt.Printf("\n")

	switch {

	case 1 > 2:
		fmt.Println("1st case")
	case 1 < 3:
		fmt.Println("2nd case")
	}

}
